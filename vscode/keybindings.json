// Place your key bindings in this file to overwrite the defaults
[
    { "key": "cmd+t",         "command": "workbench.action.tasks.runTask", 
                                 "args": "test" },
    { "key": "cmd+shift+t",   "command": "go.test.cursor", 
                                 "when": "editorTextFocus"},
    { "key": "cmd+r",         "command": "workbench.action.tasks.runTask", 
                                 "args": "run" },
    { "key": "cmd+enter",     "command": "workbench.action.terminal.toggleTerminal" },
    { "key": "cmd+d",  "command": "workbench.action.terminal.split",
                                 "when": "terminalFocus" },
]
